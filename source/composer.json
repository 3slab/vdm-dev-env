{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "3slab/vdm-healthcheck-bundle": "dev-master",
    "3slab/vdm-library-bundle": "dev-master",
    "3slab/vdm-library-doctrine-transport-bundle": "dev-master",
    "3slab/vdm-library-http-transport-bundle": "dev-master",
    "3slab/vdm-prometheus-bundle": "dev-master",
    "doctrine/mongodb-odm-bundle": "^4.3",
    "koco/messenger-kafka": "0.13",
    "nyholm/psr7": "^1.4",
    "symfony/amqp-messenger": "5.2.*",
    "symfony/cache": "5.2.*",
    "symfony/console": "5.2.*",
    "symfony/dotenv": "5.2.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.2.*",
    "symfony/monolog-bundle": "^3.6",
    "symfony/orm-pack": "*",
    "symfony/yaml": "5.2.*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.2.*"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../VdmLibraryBundle"
    },
    {
      "type": "path",
      "url": "../VdmLibraryHttpTransportBundle"
    },
    {
      "type": "path",
      "url": "../VdmLibraryDoctrineTransportBundle"
    },
    {
      "type": "path",
      "url": "../VdmHealthcheckBundle"
    },
    {
      "type": "path",
      "url": "../VdmPrometheusBundle"
    }
  ]
}
